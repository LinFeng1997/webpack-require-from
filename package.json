  "description": "Webpack plugin that allows to configure path or URL for fetching dynamic imports",
  "main": "index.js",
  "scripts": {
    "build-webpack2": "cd ./test/webpack2 && yarn run build && cd ..",
    "build-webpack3": "cd ./test/webpack3 && yarn run build && cd ..",
