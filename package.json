    "build-tests": "yarn run build-webpack4 && yarn run build-webpack3 && yarn run build-webpack2",
    "test": "yarn run build-tests && mocha ./test/common/tests",
    "test-slim": "mocha ./test/common/tests"
  },
  "peerDependencies": {
    "tapable": "^1.0.0"
